{
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "Node",
    "target": "ESNext",
    "module": "NodeNext",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "noEmit": true,
    "allowJs": true,
    "stripInternal": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "paths": {
      "react-node-insim": ["./src/index"]
    }
  }
}
